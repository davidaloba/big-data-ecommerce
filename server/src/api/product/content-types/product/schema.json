{
  "kind": "collectionType",
  "collectionName": "product",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "required": true,
      "type": "uid"
    },
    "blocks": {
      "type": "dynamiczone",
      "required": true,
      "components": [
        "shop.product"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "featuredImages": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "float",
      "required": true,
      "default": 0
    },
    "content": {
      "type": "component",
      "repeatable": false,
      "component": "shop.product-content"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "__lib.seo"
    }
  }
}
