{
  "kind": "collectionType",
  "collectionName": "collection",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "collection"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "featuredImage": {
      "type": "media",
      "required": true,
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "__lib.link"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category"
    }
  }
}
