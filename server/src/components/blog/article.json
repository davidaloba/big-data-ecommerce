{
  "collectionName": "components_blog_article",
  "info": {
    "displayName": "Article",
    "icon": "align-left",
    "description": ""
  },
  "options": {},
  "attributes": {
    "header": {
      "type": "component",
      "repeatable": false,
      "component": "__lib.header"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "content": {
      "type": "component",
      "repeatable": false,
      "component": "blog.article-content"
    },
    "related": {
      "type": "component",
      "repeatable": false,
      "component": "blog.related-articles"
    },
    "topics": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::topic.topic",
      "inversedBy": "articles"
    },
    "editors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    }
  }
}
