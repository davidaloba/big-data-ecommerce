{
  "collectionName": "components_modules_product",
  "info": {
    "displayName": "Product",
    "icon": "align-left",
    "description": ""
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "shop"
    },
    "tag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tag.tag",
      "inversedBy": "shop"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
      
    },
    "content": {
      "type": "component",
      "repeatable": false,
      "component": "product.product-content"
    },
    "reviews": {
      "type": "component",
      "repeatable": false,
      "component": "product.product-reviews"
    },
    "related": {
      "type": "component",
      "repeatable": false,
      "component": "product.related-products"
    }
  }
}
